{
  "name": "stack-server-long-polling",
  "version": "1.0.0",
  "description": "FIFO Stack Server Project",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "LOG_MODE=info node src/index.js",
    "debug": "LOG_MODE=debug node --inspect src/index.js",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint . --ext .js",
    "lint:fix": "ESLINT_USE_FLAT_CONFIG=false eslint . --ext .js --fix",
    "test:vitest": "vitest --config=tests/vitest.config.js",
    "typecheck": "tsc --noEmit",
    "build": "tsc -p tsconfig.json",
    "start:dev": "LOG_MODE=info tsx watch src/index.js", 
    "debug:dev": "LOG_MODE=debug tsx --inspect src/index.js", 
    "start:prod": "LOG_MODE=info node --enable-source-maps dist/index.js"
  },
  "repository": {
    "type": "git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^24.5.2",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-const-case": "^1.2.2",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-jsdoc": "^50.6.17",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-unicorn": "^59.0.1",
    "jest": "^29.7.0",
    "jest-date-mock": "^1.0.10",
    "prettier": "^3.5.3",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0"
  }
}
